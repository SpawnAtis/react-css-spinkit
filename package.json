{
  "name": "react-css-spinkit",
  "version": "1.0.0",
  "description": "CSS-only spinners for React",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint:es": "eslint .",
    "lint:es:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:es:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpawnAtis/react-css-spinkit.git"
  },
  "keywords": [
    "react",
    "react-css-spinkit"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpawnAtis/react-css-spinkit/issues"
  },
  "homepage": "https://github.com/SpawnAtis/react-css-spinkit#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.15.3",
    "eslint-config-kentcdodds": "^14.3.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {}
